{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "Workflow-Processor",
      "Environment": "Staging-Prod"
    }
  },
  "AllowedHosts": "*",
  "WorkflowProcessor": {
    "RegisterImages" : true,
    "RefreshFlatManifestations" : false,
    "RebuildPackageCaches" : false,
    "RebuildTextCaches" : false,
    "RebuildAllAnnoPageCaches" : false
  }
}