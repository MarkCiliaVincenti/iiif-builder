@model SettingsModel

@{
    ViewBag.Title = "Settings";
}

<a id="top"></a>

<div class="col-md-12">
    <h3 style="margin: 25px 0 20px 0">Environment AppSettings</h3>
</div>

<div class="row">
    <div class="col-md-12">
        <pre>
            @Model.AppSettingsForEnvironment
        </pre>
    </div>
</div>

<div class="col-md-12">
    <h3 style="margin: 25px 0 20px 0">Base AppSettings</h3>
</div>

<div class="row">
    <div class="col-md-12">
        <pre>
            @Model.AppSettings
        </pre>
    </div>
</div>

