@using Utils;
@model Wellcome.Dds.Server.Models.LoginModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en" data-placeholder-focus="false" class=" has-js">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>Wellcome Library | Restricted Digital Content</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no">
    <script>document.getElementsByTagName('html')[0].className += ' has-js';</script>
    <link rel="stylesheet" href="/roleproviderlogin/assets/sso-core.min.css">
    <link rel="stylesheet" media="all and (min-width: 50em)" href="/roleproviderlogin/assets/sso-layout.min.css">
    <!--[if (lt IE 9)&(!IEMobile)]><link rel="stylesheet" media="screen" href="/cas/css/sso-layout.min.css" /><![endif]-->
    <link rel="stylesheet" href="/roleproviderlogin/assets/sso.min.css">
    <link rel="stylesheet" media="print" class="contrast-css" href="/roleproviderlogin/assets/sso-print.min.css">
</head>
<body class="logged-in theme theme--">
    <div class="page-wrap">
        <header>
            <div class="section header" role="banner">
                <div class="wrap">
                    <ul class="top-nav" role="navigation">
                        <li class="account-links"><a class="join button" href="#" onclick="window.close()">Close this window</a></li>
                    </ul>
                </div>
                <div class="wrap">
                    <div class="group relative">
                        <div class="site-title wellcome-trust-title">
                            <a href="https://wellcomelibrary.org">
                                <img src="/roleproviderlogin/images/wellcome-logo.png" alt="Wellcome Trust"
                                     width="145" height="58">
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </header>
        <section>
            <div class="section">
                <div class="grid-pattern page-heading">
                    <div class="wrap">
                        <div class="group fixed-left">
                            <div class="fixed-column">
                                <div class="section-title">
                                    <h2>Account</h2>
                                </div>
                            </div>
                            <div class="fluid-column">
                                <h1 class="page-title">Restricted Content</h1>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="wrap">
                    <div class="group fixed-left">
                        <div class="fixed-column"></div>
                        <div class="fluid-column">
                            <div class="group fixed-right">
                                <div class="fluid-column">
                                    <div>
                                    </div>
                                    <div class="callout group login">
                                        <form id="fm1" action="" method="post">
                                            <p class="field">
                                                <label for="username"><span class="accesskey">U</span>sername or Library card number</label>
                                                <input id="username" name="directlogin_u" tabindex="1" accesskey="u" type="text" value="@Model.Username" size="25" autocomplete="off">
                                            </p>
                                            <div id="ipssopinentry">
                                                <p style="position:relative;" class="field">
                                                    <label for="password"><span class="accesskey">P</span>assword:</label>
                                                    <input id="password" name="directlogin_p" class="js-hide-show-password" tabindex="2" accesskey="p" type="password"
                                                           value="" size="25" autocomplete="off">
                                                    <span class="hideshow js-hide-show-link">Show</span>
                                                </p>
                                            </div>
                                            @if (Model.Message.HasText())
                                            {
                                                <p style="color: darkred">
                                                    @Model.Message
                                                </p>
                                            }
                                            <p class="group">
                                                @*<input type="hidden" name="lt" value="LT-2512-qvIfBJyxORoY9qTexiU0TBajq4LjMw-wt-brush">
                                                    <input type="hidden" name="execution" value="c02b4716-0470-4787-a326-873483dd4366_" />
                                                    <input type="hidden" name="_eventId" value="submit">*@
                                                <input class="btn-submit float-right" name="submit" accesskey="l" value="LOGIN" tabindex="4" type="submit">
                                            </p>
                                            <div>
                                            </div>
                                        </form>
                                    </div>
                                    <ul class="plain-list">
                                        <li><a class="action" href="https://catalogue.wellcomelibrary.org/patroninfo/">Forgot username?</a></li>
                                        <li><a class="action" href="https://catalogue.wellcomelibrary.org/pinreset">Forgot password?</a></li>
                                        <li><a class="action" href="https://catalogue.wellcomelibrary.org/patroninfo">Register</a></li>
                                    </ul>
                                    <br>
                                    @*<div class="callout group">
                                            <div class="beside-social-logins">
                                                <p><strong>Anyone can log in here to:</strong></p>
                                                <ul>
                                                    <li>see saved items from the digital player</li>
                                                    <li>access restricted digital content</li>
                                                </ul>
                                                <p>More text here...</p>
                                            </div>
                                        </div>*@
                                    <p>Having trouble logging in to your account? <a href="mailto:library@wellcome.ac.uk" tabindex="75">Get in touch</a>.</p>
                                    <br>
                                </div>
                                <div class="fixed-column">
                                    <div class="search-tips">
                                        <h3>Login tips</h3>
                                        <p>To view restricted content, you'll need to log in with your Library card number or Library username.</p>
                                        <p>If you don't have a Library card, <a href="https://catalogue.wellcomelibrary.org/patroninfo">register</a> for an account.</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <footer>
            <div class="section footer">
                <div class="wrap">
                    <div class="group third">
                        <div class="column two-thirds">
                            <p><small><a href="https://wellcome.ac.uk/about-us/privacy-and-terms" target="_blank">Privacy and terms</a></small></p>
                            <p><small>© 2020 Wellcome Trust</small></p>
                        </div><!-- /.column.two-thirds -->
                        <div class="column text-right wellcome-trust">
                            <a href="https://wellcomelibrary.org/" title="Visit the Wellcome Library">
                                <img src="/roleproviderlogin/images/wellcome-logo.png"
                                     alt="Wellcome Library" width="145" height="58">
                            </a>
                        </div><!-- /.column -->
                    </div><!-- /.group.third -->
                </div><!-- /.wrap -->
            </div><!-- /.section.footer -->
        </footer>
        <script src="/roleproviderlogin/assets/jquery.min.js"></script>
        <script src="/roleproviderlogin/assets/sso-pack.min.js"></script>
    </div>
</body>
</html>

