{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "System": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Properties": {
      "ApplicationName": "DlcsJob-Processor"
    }
  },
  "AllowedHosts": "*",
  "JobProcessor": {
    "YieldTimeSecs": 60,
    "Filter": "",
    "Mode": "processqueue"
  }
}